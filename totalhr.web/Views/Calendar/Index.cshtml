@model List<totalhr.data.EF.Calendar>

@{
    ViewBag.Title = "Index";
}

<div id="main">
    <div class="content">
        <h1>Company Calendars</h1>
        
        @foreach (var calender in Model)
        {
            <p>
                @calender.Name
                <a href="/Calendar/GenerateDefault/@calender.id" title="@calender.Name">@calender.Name</a>
            </p>
            
        }
        
    </div>
  </div>



