<!DOCTYPE html>
<html @RenderSection("htmlattrib", required: false) >
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css/popup.css")
    @RenderSection("extrastyles", required: false)
    
    @Scripts.Render("~/Scripts/jquery-1.11.0.min.js")
    @Scripts.Render("~/Scripts/Site/common.js")

    @RenderSection("headerScripts", required:false)
</head>
<body>
   <div id="main" style="background:red;height:70%;overflow:hidden;">        
        <div class="popuphead">             
            @RenderSection("head2", required: true)   
            <div id="toolbox">
                <span class="spclose" onclick="parent.ClosePopup('dvPopup')">&nbsp;</span> 
            </div>
        </div> 
        
        <div id="popupBody" style="height:70%;overflow:hidden;">
            @RenderBody()
        </div>
        
       <div id="popupfooter">
           footer
       </div>
 </div>


    @RenderSection("scripts", required: false)

   <div id="dvPopupAjax" style="display:none;"></div>

</body>
</html>
